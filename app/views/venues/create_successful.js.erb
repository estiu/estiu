<% select_content = nil %>

<% form_for(CampaignDraft.new, class: 'form-horizontal') do |f| %> 
  <% form_fields(f, 'campaign_drafts.form') do |field| %>
    <% select_content = render 'campaign_drafts/venue_select', field: field, selected_id: selected_id %>
  <% end %>
<% end %>

$('#<%= campaign_draft_venue_row_id %>').html("<%= j(select_content) %>")
$('#<%= new_venue_form_modal_id %>').modal('hide')
$('.<%= new_venue_form_class %>')[0].reset()